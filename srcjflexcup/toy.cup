package it.esercitazione4;

import java.util.ArrayList;
import it.esercitazione4.visitor.*;
import java_cup.runtime.*;

parser code {:

       public VisitableNode<Node> makeNode(String name,Object value, VisitableNode<Node> ... childrens){
                Node node = new Node(name,value);
                VisitableNode<Node> toReturn = new VisitableNode<Node>(node);
               for(int i = 0; i < childrens.length; i ++){
                   toReturn.add(childrens[i]);
               }
               return toReturn;
           }

         public VisitableNode<Node> makeNode(String name,Object value, ArrayList<VisitableNode<Node>> childrens){

                    Node node = new Node(name,value);
                    VisitableNode<Node> toReturn = new VisitableNode<Node>(node);

                    for (VisitableNode<Node> nodeChild : childrens)
                       toReturn.add(nodeChild);

                   return toReturn;
            }
